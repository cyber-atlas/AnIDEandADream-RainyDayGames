<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.iastate.loginscreen">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name="edu.iastate.IDE_AND_A_DREAM.Login.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.iastate.IDE_AND_A_DREAM.secondpage" />
        <activity android:name="edu.iastate.IDE_AND_A_DREAM.Login.signUp" />
        <activity android:name="edu.iastate.IDE_AND_A_DREAM.Snake.GamesList" />
        <activity android:name="edu.iastate.IDE_AND_A_DREAM.Snake.SnakeSplash" />
        <activity android:name="edu.iastate.IDE_AND_A_DREAM.Snake.SnakeStartup" />
        <activity android:name="edu.iastate.IDE_AND_A_DREAM.Snake.SnakeMainActivity"></activity>
    </application>

</manifest>